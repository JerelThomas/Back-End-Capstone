{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 75
      },
      "email": {
        "type": "string",
        "minLength": 1,
        "maxLength": 75
      },
      "phone_number": {
        "type": "string"
      },
      "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "isauth": {
        "type": "boolean"
      },
      "first_name":{
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "last_name":{
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "location":
      {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "email",
      "phone_number",
      "password",
      "location",
      "isauth",
      "first_name",
      "last_name"

    ]
  }
  